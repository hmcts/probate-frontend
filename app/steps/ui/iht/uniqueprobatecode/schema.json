{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "unique-probate-code",
  "type": "object",
  "properties": {
    "uniqueProbateCodeId": {
      "type": "string",
      "minLength": 21,
      "maxLength": 24,
      "pattern": "^(cts|CTS)\\s?([a-zA-Z0-9]\\s?){18}$"
    }
  },
  "required": [
    "uniqueProbateCodeId"
  ]
}

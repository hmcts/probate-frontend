{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "deceased-dod",
  "type": "object",
  "properties": {
    "dod_day": {
      "type": "integer",
      "minimum": 1,
      "maximum": 31
    },
    "dod_month": {
      "type": "integer",
      "minimum": 1,
      "maximum": 12
    },
    "dod_year": {
      "type": "integer",
      "minimum": 1000,
      "maximum": 9999
    },
    "dod_date": {
      "type": "string",
      "format": "date-time"
    }
  },
  "required": [
    "dod_day",
    "dod_month",
    "dod_year"
  ]
}

{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "executors-names",
  "type": "object",
  "properties": {
    "executorName": {
      "type": "array",
      "minItems": 1,
      "maxItems": 19,
      "items" :
        {
          "type": "string",
          "minLength": 2,
          "pattern": "^[^<>]+$"
        }
    }
  },
  "required": [
    "executorName"
  ]
}

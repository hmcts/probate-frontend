{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "executors-names",
  "type": "object",
  "properties": {
    "executorName": {
      "type": "array",
      "minItems": 1,
      "maxItems": 19,
      "items":
        {
          "type": "string",
          "minLength": 2,
          "pattern": "^[\\sa-zA-Z'-]+$"
        }
    }
  },
  "required": [
    "executorName"
  ]
}
